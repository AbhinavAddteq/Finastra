{
  "key": "${addon.key}",
  "baseUrl": "${addon.base-url}",
  "name": "Cloudtest",
  "authentication": {
    "type": "jwt"
  },
  "lifecycle": {
    "installed": "/installed",
    "uninstalled": "/uninstalled"
  },

  "apiMigrations": {
    "signed-install": true
  },

  "scopes": ["READ", "ACT_AS_USER"],

  "modules": {
    "dialogs": [
      {
        "url": "/viewFieldValueDialog",
        "options": {
          "size": "medium",
          "header": {
            "value": "View Field Values"
          }
        },
        "key": "view-field-value-dialog"
      }
    ],

    "webItems": [
      {
        "name": {
          "value": "View field value"
        },
        "location": "jira.project.sidebar.plugins.navigation",
        "weight": 200,
        "key": "view-field-value",
        "target": {
          "type": "dialogmodule",
          "options": {
            "key": "view-field-value-dialog"
          }
        }
      }
    ]
  }
}
